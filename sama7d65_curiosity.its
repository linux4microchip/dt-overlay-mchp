	configurations {
		default = "kernel_dtb";

		kernel_dtb {
			description = "Linux kernel and base FDT blob for SAMA7D65-CURIOSITY board";
			kernel = "kernel";
			fdt = "base_fdt";
		};

		base_dtb {
